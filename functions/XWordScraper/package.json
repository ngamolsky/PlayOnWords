{
  "name": "functions",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "build": "tsc",
    "start": "concurrently \"tsc -w\" \"nodemon --watch ./lib/ -- exec node -r yamlenv/config node_modules/@google-cloud/functions-framework --source=lib/ --target=XWordScraper\" ",
    "deploy": "gcloud functions deploy XWordScraper --runtime=nodejs16 --trigger-http"
  },
  "main": "lib/index.js",
  "dependencies": {
    "axios": "^0.21.1",
    "firebase-admin": "^10.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "~1.9.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^8.5.0",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "typescript": "^4.2.3",
    "yamlenv": "^1.0.0"
  },
  "private": true
}
